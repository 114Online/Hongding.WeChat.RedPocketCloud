@model TemplateViewModel
@{ 
    Layout = null;
}
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>咻一咻抽红包</title>
    <link href="~/assets/css/shoop.css" rel="stylesheet" />
    <script src="~/assets/js/jquery-1.10.2.js"></script>
    <script src="~/assets/js/jquery.cookie.min.js"></script>
    <script>
        var Merchant = '@ViewContext.RouteData.Values["Merchant"].ToString()'; limiting = @ViewBag.Limit;
    </script>
    <script src="~/assets/js/shoop.js"></script>
</head>
<body>
    <img class="loading" src="~/assets/img/loading.svg" />
